Tab13:AddSection({"Efeitos RGB"})

-- RGB Nome
local rgbName = false
Tab13:AddToggle({
    Name = "RGB no Nome",
    Default = false,
    Callback = function(state)
        rgbName = state
        if state then
            while rgbName do
                local randomColor = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                ReplicatedStorage.RE:FindFirstChild("1RPNam1eColo1r"):FireServer("PickingRPNameColor", randomColor)
                wait(0.1)
            end
        end
    end
})

-- RGB Bio
local rgbBio = false
Tab13:AddToggle({
    Name = "RGB na Bio",
    Default = false,
    Callback = function(state)
        rgbBio = state
        if state then
