Tab3:AddSection({"Copiar Avatar"})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Remotes = ReplicatedStorage:WaitForChild("Remotes")

local selectedPlayerForCopy = nil

-- Lista de jogadores
local function GetPlayerNames()
    local playerNames = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player.Name ~= LocalPlayer.Name then
            table.insert(playerNames, player.Name)
        end
    end
    return playerNames
end

local PlayerDropdown = Tab3:AddDropdown({
    Name = "Lista de Jogadores",
    Description = "Selecione um jogador para copiar",
    Options = GetPlayerNames(),
    Default = "",
    Callback = function(playername)
        selectedPlayerForCopy = playername
    end
})

-- Atualizar lista
Tab3:AddButton({
    Name = "Atualizar Lista",
    Callback = function()
        PlayerDropdown:Set(GetPlayerNames())
    end
})

-- Copiar Avatar
Tab3:AddButton({
    Name = "Copiar Avatar",
    Callback = function()
        if not selectedPlayerForCopy then return end

        local targetPlayer = Players:FindFirstChild(selectedPlayerForCopy)
        if targetPlayer and targetPlayer.Character then
            local targetHumanoid = targetPlayer.Character:FindFirstChildOfClass("Humanoid")
            local localHumanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            
            if targetHumanoid and localHumanoid then
                local targetDesc = targetHumanoid:GetAppliedDescription()
                
                -- Aplicar corpo
                local argsBody = {
                    [1] = {
                        targetDesc.Head or 0,
                        targetDesc.Torso or 0,
                        targetDesc.RightArm or 0,
                        targetDesc.LeftArm or 0,
                        targetDesc.RightLeg or 0,
                        targetDesc.LeftLeg or 0
                    }
                }
                Remotes.ChangeCharacterBody:InvokeServer(unpack(argsBody))
                
                -- Aplicar roupas
                if targetDesc.Shirt ~= 0 then
                    Remotes.Wear:InvokeServer(targetDesc.Shirt)
                end
                if targetDesc.Pants ~= 0 then
                    Remotes.Wear:InvokeServer(targetDesc.Pants)
                end
                
                -- Aplicar acess√≥rios
                for _, accessory in ipairs(targetDesc:GetAccessories(true)) do
                    if accessory.AssetId ~= 0 then
                        Remotes.Wear:InvokeServer(accessory.AssetId)
                    end
                end
            end
        end
    end
})

-- Avatares 3D
Tab3:AddSection({"Avatares 3D"})

local Avatares3D = {
    {Nome = "Gato de Manga", ID = 124948425515124},
    {Nome = "Dinossauro", ID = 11941741105},
    {Nome = "Coelho", ID = 71797333686800},
    {Nome = "Pinguim", ID = 94944293759578},
    {Nome = "Among Us", ID = 18234669337}
}

local selectedAvatar = nil

local AvatarDropdown = Tab3:AddDropdown({
    Name = "Avatares 3D",
    Description = "Selecione um avatar",
    Options = {unpack(function()
        local names = {}
        for _, avatar in ipairs(Avatares3D) do
            table.insert(names, avatar.Nome)
        end
        return names
    end())},
    Callback = function(selected)
        selectedAvatar = selected
    end
})

Tab3:AddButton({
    Name = "Equipar Avatar",
    Callback = function()
        if not selectedAvatar then return end
        
        for _, avatar in ipairs(Avatares3D) do
            if avatar.Nome == selectedAvatar then
                Remotes.Wear:InvokeServer(avatar.ID)
                break
            end
        end
    end
})

