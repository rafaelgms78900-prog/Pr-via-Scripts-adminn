    ["Casa 2"] = Vector3.new(234.49, 4.37, 228.00),
    ["Casa 3"] = Vector3.new(262.79, 21.37, 210.84),
    ["Casa 4"] = Vector3.new(229.60, 21.37, 225.40),
    ["Casa 5"] = Vector3.new(173.44, 21.37, 228.11),
    ["Casa 6"] = Vector3.new(-43, 21, -137),
    ["Casa 7"] = Vector3.new(-40, 36, -137)
}

local selectedHouse = nil

local HouseDropdown = Tab4:AddDropdown({
    Name = "Selecionar Casa",
    Options = {"Casa 1", "Casa 2", "Casa 3", "Casa 4", "Casa 5", "Casa 6", "Casa 7"},
    Callback = function(value)
        selectedHouse = value
    end
})

Tab4:AddButton({
    Name = "Teleportar para Casa",
    Callback = function()
        if selectedHouse and houses[selectedHouse] then
            local player = game.Players.LocalPlayer
            if player.Character then
                player.Character.HumanoidRootPart.CFrame = CFrame.new(houses[selectedHouse])
            end
        end
    end
})

-- Auto Unban
local isUnbanActive = false
Tab4:AddToggle({
    Name = "Auto Unban",
    Description = "Remove ban automaticamente das casas",
    Default = false,
    Callback = function(state)
        isUnbanActive = state
        if state then
            while isUnbanActive do
                pcall(function()
                    for _, v in pairs(workspace:WaitForChild("001_Lots"):GetDescendants()) do
                        if v.Name:match("^BannedBlock") then
                            v:Destroy()
                        end
                    end
                end)
                wait(1)
            end
        end
    end
})

