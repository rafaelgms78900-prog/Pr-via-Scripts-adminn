Tab5:AddSection({"Controle de Carros"})

local selectedVehicle = nil

-- Fun√ß√£o para obter lista de carros
local function getVehicleList()
    local vehicles = {}
    local vehiclesFolder = workspace:FindFirstChild("Vehicles")
    if vehiclesFolder then
        for _, vehicle in ipairs(vehiclesFolder:GetChildren()) do
            if vehicle.Name:match("Car$") then
                table.insert(vehicles, vehicle.Name)
            end
        end
    end
    return vehicles
end

local VehicleDropdown = Tab5:AddDropdown({
    Name = "Selecionar Carro",
    Options = getVehicleList(),
    Callback = function(vehicleName)
        selectedVehicle = vehicleName
    end
})

-- Atualizar lista de carros
Tab5:AddButton({
    Name = "Atualizar Lista",
    Callback = function()
        VehicleDropdown:Set(getVehicleList())
    end
})

-- Trazer Carro
Tab5:AddButton({
    Name = "Trazer Carro",
    Callback = function()
        if not selectedVehicle then return end
        
        local vehiclesFolder = workspace:FindFirstChild("Vehicles")
        if not vehiclesFolder then return end
        
        local vehicle = vehiclesFolder:FindFirstChild(selectedVehicle)
        if vehicle and vehicle.PrimaryPart then
            local player = game.Players.LocalPlayer
            if player.Character then
                vehicle:SetPrimaryPartCFrame(player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 5))
            end
        end
    end
})

-- Deletar Carro
Tab5:AddButton({
    Name = "Deletar Carro",
    Callback = function()
        if not selectedVehicle then return end
        
        local vehiclesFolder = workspace:FindFirstChild("Vehicles")
        if not vehiclesFolder then return end
        
        local vehicle = vehiclesFolder:FindFirstChild(selectedVehicle)
        if vehicle then
            vehicle:Destroy()
        end
    end
})

-- Deletar Todos os Carros
Tab5:AddButton({
    Name = "Deletar Todos os Carros",
    Callback = function()
        local vehiclesFolder = workspace:FindFirstChild("Vehicles")
        if vehiclesFolder then
            for _, vehicle in ipairs(vehiclesFolder:GetChildren()) do
                if vehicle.Name:match("Car$") then
                    vehicle:Destroy()
                end
            end
        end
    end
})

