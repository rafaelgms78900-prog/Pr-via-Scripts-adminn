Tab8:AddSection({"M√∫sica Pessoal"})

local personalMusicID = ""

Tab8:AddTextBox({
    Name = "ID da M√∫sica",
    PlaceholderText = "Digite o ID da m√∫sica",
    Callback = function(value)
        personalMusicID = value
    end
})

-- Tocar M√∫sica
Tab8:AddButton({
    Name = "Tocar M√∫sica",
    Callback = function()
        if personalMusicID ~= "" then
            -- R√°dio
            ReplicatedStorage.RE:FindFirstChild("PlayerToolEvent"):FireServer("ToolMusicText", personalMusicID)
            -- Casa
            ReplicatedStorage.RE:FindFirstChild("1Player1sHous1e"):FireServer("PickHouseMusicText", personalMusicID)
            -- Carro
            ReplicatedStorage.RE:FindFirstChild("1Player1sCa1r"):FireServer("PickingCarMusicText", personalMusicID)
        end
    end
})

-- Parar M√∫sica
Tab8:AddButton({
    Name = "Parar M√∫sica",
    Callback = function()
        ReplicatedStorage.RE:FindFirstChild("PlayerToolEvent"):FireServer("ToolMusicText", "")
        ReplicatedStorage.RE:FindFirstChild("1Player1sHous1e"):FireServer("PickHouseMusicText", "")
        ReplicatedStorage.RE:FindFirstChild("1Player1sCa1r"):FireServer("PickingCarMusicText", "")
    end
})

--------------------------------------------------------------------------------------------------------------------------------
                                                   -- === TAB 9: TROLL === --
---------------------------------------------------------------------------------------------------------------------------------

Tab9:AddSection({"Ferramentas de Troll"})

local selectedPlayerForTroll = nil

-- Lista de jogadores para troll
local TrollPlayerDropdown = Tab9:AddDropdown({
    Name = "Selecionar Jogador",
    Options = GetPlayerNames(),
    Callback = function(playerName)
        selectedPlayerForTroll = playerName
    end
})

-- Atualizar lista
Tab9:AddButton({
    Name = "Atualizar Lista",
    Callback = function()
        TrollPlayerDropdown:Set(GetPlayerNames())
    end
})

-- Teleportar para Jogador
Tab9:AddButton({
    Name = "Teleportar para Jogador",
    Callback = function()
        if not selectedPlayerForTroll then return end
        
        local targetPlayer = Players:FindFirstChild(selectedPlayerForTroll)
        if targetPlayer and targetPlayer.Character then
            local player = game.Players.LocalPlayer
            if player.Character then
                player.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
            end
        end
    end
})

-- Fling com Bola
Tab9:AddButton({
    Name = "Fling com Bola",
    Callback = function()
        if not selectedPlayerForTroll then return end
        
        -- Equipar bola
        ReplicatedStorage.RE:FindFirstChild("1Too1l"):InvokeServer("PickingTools", "SoccerBall")
        wait(0.5)
        
        -- Deletar da mochila para spawnar no mundo
        ReplicatedStorage.RE:FindFirstChild("1Clea1rTool1s"):FireServer("PlayerWantsToDeleteTool", "SoccerBall")
    end
})

-- Espionar Jogador
local isSpectating = false
Tab9:AddToggle({
    Name = "Espionar Jogador",
    Default = false,
    Callback = function(state)
        isSpectating = state
        if state and selectedPlayerForTroll then
            local targetPlayer = Players:FindFirstChild(selectedPlayerForTroll)
            if targetPlayer and targetPlayer.Character then
                workspace.CurrentCamera.CameraSubject = targetPlayer.Character:FindFirstChildOfClass("Humanoid")
            end
        else
            local player = game.Players.LocalPlayer
            if player.Character then
                workspace.CurrentCamera.CameraSubject = player.Character:FindFirstChildOfClass("Humanoid")
            end
        end
    end
})

--------------------------------------------------------------------------------------------------------------------------------
                                                   -- === TAB 10: LAG SERVER === --
---------------------------------------------------------------------------------------------------------------------------------

Tab10:AddSection({"Lag no Servidor"})

-- Lag com Laptop
local lagLaptop = false
Tab10:AddToggle({
    Name = "Lag com Laptop",
    Default = false,
    Callback = function(state)
        lagLaptop = state
        if state then
            local laptop = workspace:FindFirstChild("WorkspaceCom"):FindFirstChild("001_GiveTools"):FindFirstChild("Laptop")
            if laptop then
                while lagLaptop do
                    local player = game.Players.LocalPlayer
                    if player.Character then
                        player.Character.HumanoidRootPart.CFrame = laptop.CFrame
                        fireclickdetector(laptop.ClickDetector)
                    end
                    wait(0.0001)
                end
            end
        end
    end
})

-- Lag com Bomba
local lagBomb = false
Tab10:AddToggle({
    Name = "Lag com Bomba",
    Default = false,
    Callback = function(state)
        lagBomb = state
        if state then
            local bomb = workspace:FindFirstChild("WorkspaceCom"):FindFirstChild("001_CriminalWeapons"):FindFirstChild("GiveTools"):FindFirstChild("Bomb")
            if bomb then
                while lagBomb do
                    local player = game.Players.LocalPlayer
                    if player.Character then
                        player.Character.HumanoidRootPart.CFrame = bomb.CFrame
                        fireclickdetector(bomb.ClickDetector)
                    end
                    wait(0.0001)
                end
            end
        end
    end
})

