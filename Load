-- Fun√ß√£o para disparar eventos
local function fireEvent(event, ...)
    if event then
        event:FireServer(...)
    end
end

-- Configura√ß√£o do nome e bio
fireEvent(NameTextEvent, "RolePlayName", "Itoshi Hub V2")
fireEvent(NameColorEvent, "PickingRPNameColor", Color3.new(0, 0, 0))
fireEvent(NameTextEvent, "RolePlayBio", "Criador || Itoshi ||")
fireEvent(NameColorEvent, "PickingRPBioColor", Color3.new(1, 0, 0))

