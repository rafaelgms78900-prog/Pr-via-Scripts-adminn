local currentAudioID = ""
local loopAudio = false

Tab7:AddTextBox({
    Name = "ID do √Åudio",
    Description = "Digite o ID do √°udio",
    PlaceholderText = "ex: 123456789",
    Callback = function(text)
        currentAudioID = text
    end
})

-- Tocar √Åudio
Tab7:AddButton({
    Name = "Tocar √Åudio",
    Callback = function()
        if currentAudioID ~= "" then
            local args = {
                [1] = workspace,
                [2] = tonumber(currentAudioID),
                [3] = 1
            }
            ReplicatedStorage.RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
        end
    end
})

-- Loop √Åudio
Tab7:AddToggle({
    Name = "Loop √Åudio",
    Default = false,
    Callback = function(state)
        loopAudio = state
        if state then
            while loopAudio do
                if currentAudioID ~= "" then
                    local args = {
                        [1] = workspace,
                        [2] = tonumber(currentAudioID),
                        [3] = 1
                    }
                    ReplicatedStorage.RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
                end
                wait(3)
            end
        end
    end
})

