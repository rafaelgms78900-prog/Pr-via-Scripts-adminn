LoadingFrame.Size = UDim2.new(0.5, 0, 0, 30)
LoadingFrame.Position = UDim2.new(0.25, 0, 0.85, 0)
LoadingFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
