Tab12:AddSection({"Locais do Mapa"})

local locations = {
    ["Pra√ßa"] = Vector3.new(-26.17, 3.48, -0.93),
    ["Banco"] = Vector3.new(1.99, 3.32, 236.65),
    ["Hospital"] = Vector3.new(-303.2, 3.40, 13.74),
    ["Shopping"] = Vector3.new(151.05, 3.52, -190.64),
    ["Aeroporto"] = Vector3.new(290.23, 4.32, 42.57),
    ["Hotel"] = Vector3.new(159.10, 3.32, 164.97),
    ["Praia"] = Vector3.new(55.69, 2.94, -1403.60)
}

local selectedLocation = "Pra√ßa"

local LocationDropdown = Tab12:AddDropdown({
    Name = "Locais",
    Options = {"Pra√ßa", "Banco", "Hospital", "Shopping", "Aeroporto", "Hotel", "Praia"},
    Callback = function(value)
        selectedLocation = value
    end
})

Tab12:AddButton({
    Name = "Teleportar",
    Callback = function()
        if locations[selectedLocation] then
            local player = game.Players.LocalPlayer
            if player.Character then
                player.Character.HumanoidRootPart.CFrame = CFrame.new(locations[selectedLocation])
            end
        end
    end
})

