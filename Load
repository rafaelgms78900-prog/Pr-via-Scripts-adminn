-- Conectar eventos de atualiza√ß√£o de lista de jogadores
game:GetService("Players").PlayerAdded:Connect(function()
    wait(1)
    if PlayerDropdown then PlayerDropdown:Set(GetPlayerNames()) end
    if TrollPlayerDropdown then TrollPlayerDropdown:Set(GetPlayerNames()) end
end)

game:GetService("Players").PlayerRemoving:Connect(function()
    wait(1)
    if PlayerDropdown then PlayerDropdown:Set(GetPlayerNames()) end
    if TrollPlayerDropdown then TrollPlayerDropdown:Set(GetPlayerNames()) end
end)
